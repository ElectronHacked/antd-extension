{
  "name": "antd-extension",
  "version": "0.0.1",
  "scripts": {
    "storybook": "NODE_ENV=development start-storybook -p 9002 --debug-webpack",
    "start": "npm run storybook",
    "test": "react-scripts test --env=jsdom",
    "build:cjs": "lerna exec --parallel -- tsc -d --outDir lib -m commonjs",
    "build:esm": "lerna exec --parallel -- tsc -d --outDir es -m esNext ",
    "build": "npm run build:cjs && npm run build:esm",
    "bootstrap": "lerna bootstrap",
    "export-static-storybook": "build-storybook -c .storybook -o build",
    "publish-packages": "lerna publish",
    "prepublish-packages": "npm run build",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.1.3",
    "@storybook/addon-actions": "5.1.1",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-options": "^5.1.3",
    "@storybook/react": "5.1.1",
    "@types/enzyme": "^3.9.3",
    "@types/jest": "^24.0.13",
    "@types/node": "12.0.5",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/storybook__react": "^4.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.4.0",
    "lerna": "^3.14.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-scripts": "^3.0.1",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^4.0.0",
    "typescript": "3.4.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
